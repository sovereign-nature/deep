    "checks:deploy": "checkly deploy",
    "test:integration": "checkly test",
    "test:integration:dev": "playwright test --config=playwright.dev.config.ts",
    "test:integration:dev:ui": "playwright test --config=playwright.dev.config.ts --ui ",

{
  "name": "identifier",
  "version": "0.0.0",
  "private": true,
  "packageManager": "pnpm@8.6.11",
  "prettier": "@sni/prettier-config",
  "scripts": {
    "build": "turbo run build --no-daemon",
    "dev": "turbo run dev --parallel --no-daemon",
    "lint": "turbo run lint --no-daemon",
    "test": "turbo run test --no-daemon",
    "codegen": "turbo run codegen --no-daemon",
    "format": "prettier --write \"**/*.{js,ts,vue,md}\"",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{js,ts,vue}": [
      "eslint --quiet"
    ],
    "*.{json,md,js,ts,vue}": [
      "prettier --write"
    ]
  },
  "devDependencies": {
    "@sni/prettier-config": "*",
    "eslint-config-sni": "*",
    "husky": "8.0.3",
    "lint-staged": "13.2.3",
    "prettier": "3.0.1",
    "turbo": "1.8.3",
    "turbo-ignore": "1.8.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}

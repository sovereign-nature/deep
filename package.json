{
  "name": "identifier",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "test": "turbo run test",
    "codegen": "turbo run codegen",
    "format": "prettier --write \"**/*.{js,ts,vue,md}\"",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{js,ts,vue}": [
      "eslint --quiet"
    ],
    "*.{json,md,js,ts,vue}": [
      "prettier --write"
    ]
  },
  "devDependencies": {
    "eslint-config-sni": "*",
    "@sni/prettier-config": "*",
    "prettier": "2.7.1",
    "turbo": "1.4.5",
    "husky": "8.0.1",
    "lint-staged": "13.0.3"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "dependencies": {},
  "packageManager": "yarn@1.22.19"
}
